{
	"name": "ingrates-example-todo",
	"version": "0.0.0",
	"description": "an example todo app with file system persistence",
	"license": "ISC",
	"private": true,
	"eslintConfig": {
		"extends": "@little-bonsai"
	},
	"prettier": {
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true,
		"overrides": [
			{
				"files": [
					"**/*.md"
				],
				"options": {
					"useTabs": false,
					"tabWidth": 2
				}
			}
		]
	},
	"devDependencies": {
		"@little-bonsai/eslint-config": "^0.1.0",
		"eslint": "^7.32.0",
		"prettier": "^2.3.2"
	},
	"dependencies": {
		"express": "^4.17.1"
	}
}
